{
  "name": "konfi",
  "version": "0.1.9",
  "repository": "github:tobua/konfi",
  "license": "MIT",
  "author": "Matthias Giger",
  "scripts": {
    "test": "padua test",
    "start": "padua watch",
    "build": "padua build"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "burano": "^1.0.0",
    "deepmerge": "^4.2.2",
    "react-popper": "^2.2.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "main": "dist/index.js",
  "sideEffects": false,
  "source": "index.tsx",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/react": "^17.0.8",
    "padua": "^0.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "prettier": "padua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/padua/configuration/eslint.cjs"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "./tsconfig.json"
      }
    },
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": [
        "babel-jest",
        {
          "configFile": "./node_modules/padua/configuration/.babelrc"
        }
      ]
    },
    "transformIgnorePatterns": [
      "node_modules/cint"
    ]
  },
  "engines": {
    "node": ">= 14"
  }
}
